@model Result
<table>
    <tr>
        <th>Controller </th>
        <td>@Model.Controller</td>
    </tr>
    <tr>
        <th>Action </th>
        <td>@Model.Action</td>
    </tr>
    @foreach (var key in Model.Data.Keys)
    {
        <tr>
            <th>@key :</th>
            <td>@Model.Data[key]</td>
        </tr>
    }
</table>